import pandas as pd

# Загрузка данных
data = pd.read_excel('Data.xlsx')
data.columns = data.columns.str.strip()
data = data.apply(pd.to_numeric, errors='coerce')

# Список категорий для расчета
z_categories = ['Z1', 'Z2', 'Z3', 'Z4', 'Z5 (BN/SN)', 'Z5 (1 нуклеоль)']

# Словари для хранения результатов
fertilization_rates = {}
blastocyst_formation_rates = {}
high_quality_blastocyst_rates = {}

# Вычисление частоты оплодотворения для каждой категории
for z in z_categories:
    # Количество ооцитов для категории
    oocyte_column = 'Кол-во М2 ооц.'
    
    # Общее количество ооцитов для категории
    total_oocytes = data[oocyte_column] if oocyte_column in data.columns else 0

    # Оплодотворение
    fertilization_column = f'Кол-во 2PN, {z}'
    if fertilization_column in data.columns and data[fertilization_column].notna().any():
        # Частота оплодотворения
        fertilization_rates[z] = data[fertilization_column] / total_oocytes

        # Образование бластоцист
        blastocyst_column = f'Кол-во бласт. {z}'
        if blastocyst_column in data.columns and data[blastocyst_column].notna().any():
            blastocyst_formation_rates[z] = data[blastocyst_column] / data[fertilization_column]

            # Формирование бластоцист хорошего качества
            high_quality_blastocyst_column = f'Кол-во бласт (3ББ и выше) {z}'
            if high_quality_blastocyst_column in data.columns and data[high_quality_blastocyst_column].notna().any():
                high_quality_blastocyst_rates[z] = data[high_quality_blastocyst_column] / data[fertilization_column]

# Преобразование словарей в DataFrame для удобного анализа
fertilization_df = pd.DataFrame(fertilization_rates)
blastocyst_formation_df = pd.DataFrame(blastocyst_formation_rates)
high_quality_blastocyst_df = pd.DataFrame(high_quality_blastocyst_rates)

# Вычисление средних частот и стандартных отклонений, если есть данные
fertilization_mean = fertilization_df.mean() if not fertilization_df.empty else None
fertilization_sd = fertilization_df.std() if not fertilization_df.empty else None

blastocyst_formation_mean = blastocyst_formation_df.mean() if not blastocyst_formation_df.empty else None
blastocyst_formation_sd = blastocyst_formation_df.std() if not blastocyst_formation_df.empty else None

high_quality_blastocyst_mean = high_quality_blastocyst_df.mean() if not high_quality_blastocyst_df.empty else None
high_quality_blastocyst_sd = high_quality_blastocyst_df.std() if not high_quality_blastocyst_df.empty else None

# Форматирование и вывод результатов с названиями
def format_results(mean, sd, title):
    if mean is not None and sd is not None:
        formatted_results = [f"{m:.2f} ± {s:.2f}" for m, s in zip(mean, sd)]
        return pd.Series(formatted_results, index=title)
    return pd.Series(["Нет данных"] * len(mean), index=title) if mean is not None else pd.Series(["Нет данных"])

# Создание названий категорий
fertilization_titles = [f'Оплодотворение {z}' for z in z_categories if f'Кол-во 2PN, {z}' in data.columns]
blastocyst_titles = [f'Бластоцист {z}' for z in z_categories if f'Кол-во бласт. {z}' in data.columns]
high_quality_blastocyst_titles = [f'Бластоцист (3ББ и выше) {z}' for z in z_categories if f'Кол-во бласт (3ББ и выше) {z}' in data.columns]

# Вывод результатов с названиями
print("Средние частоты оплодотворения:\n", format_results(fertilization_mean, fertilization_sd, fertilization_titles))
print("\nСредние частоты образования бластоцист:\n", format_results(blastocyst_formation_mean, blastocyst_formation_sd, blastocyst_titles))
print("\nСредние частоты формирования бластоцист хорошего качества:\n", format_results(high_quality_blastocyst_mean, high_quality_blastocyst_sd, high_quality_blastocyst_titles))


from scipy import stats
from scipy.stats import f_oneway
import pandas as pd
from statsmodels.stats.multicomp import pairwise_tukeyhsd

# Функция для выполнения ANOVA
def anova_test(df):
    groups = [df[col].dropna() for col in df.columns if not df[col].empty]
    stat, p_value = f_oneway(*groups)
    return stat, p_value

# Выполнение ANOVA для всех DataFrame
results = {}

fertilization_anova_stat, fertilization_anova_p = anova_test(fertilization_df)
results['Оплодотворение'] = {'Статистика': fertilization_anova_stat, 'p-значение': fertilization_anova_p}

blastocyst_anova_stat, blastocyst_anova_p = anova_test(blastocyst_formation_df)
results['Образование бластоцист'] = {'Статистика': blastocyst_anova_stat, 'p-значение': blastocyst_anova_p}

high_quality_anova_stat, high_quality_anova_p = anova_test(high_quality_blastocyst_df)
results['Формирование бластоцист хорошего качества'] = {'Статистика': high_quality_anova_stat, 'p-значение': high_quality_anova_p}

# Создание DataFrame для вывода результатов ANOVA
anova_results_df = pd.DataFrame(results).T

# Вывод результатов ANOVA
print("Результаты ANOVA:\n", anova_results_df)
