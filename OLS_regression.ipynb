import pandas as pd
from sklearn.preprocessing import PolynomialFeatures
from sklearn.linear_model import LinearRegression
from sklearn.metrics import mean_squared_error, mean_absolute_error, r2_score

# Предположим, что 'data' - это ваш DataFrame, и 'y' - это целевая переменная
# Замените 'y' на вашу целевую переменную, если она не так названа
# Например, y = quantitative_data['Кол-во 2PN'].dropna()

# Список столбцов, для которых нужно выполнить полиномиальную регрессию
blast_columns = [
    'Кол-во бласт (3ББ и выше) Z1', 'Кол-во бласт (3ББ и выше) Z2',
    'Кол-во бласт (3ББ и выше) Z3', 'Кол-во бласт (3ББ и выше) Z4',
    'Кол-во бласт (3ББ и выше) , Z5 (BN/SN)', 'Кол-во бласт (3ББ и выше), Z5 (1 нуклеоль)'
]

# Результаты для каждой колонки
results = {}

for col in blast_columns:
    # Обработка данных
    X = quantitative_data[['Кол-во 2PN', col]].dropna()
    y = X[col]  # Определение целевой переменной
    X = X[['Кол-во 2PN']]  # Удаление целевой переменной из X

    # Полиномиальные признаки
    poly = PolynomialFeatures(degree=2)
    X_poly = poly.fit_transform(X)

    # Модель линейной регрессии
    lin_reg = LinearRegression()
    lin_reg.fit(X_poly, y)

    # Оценка модели
    y_pred = lin_reg.predict(X_poly)

    # Вычисление коэффициента детерминации и ошибок
    r2 = r2_score(y, y_pred)
    mse = mean_squared_error(y, y_pred)
    mae = mean_absolute_error(y, y_pred)

    # Сохранение результатов
    results[col] = {
        'coefficients': lin_reg.coef_,
        'intercept': lin_reg.intercept_,
        'r2': r2,
        'mse': mse,
        'mae': mae
    }

# Вывод результатов
for col, metrics in results.items():
    print(f"Столбец: {col}")
    print(f"Коэффициенты: {metrics['coefficients']}")
    print(f"Перехват: {metrics['intercept']}")
    print(f"Коэффициент детерминации (R²): {metrics['r2']}")
    print(f"Среднеквадратичная ошибка (MSE): {metrics['mse']}")
    print(f"Средняя абсолютная ошибка (MAE): {metrics['mae']}")
    print("-" * 50)

