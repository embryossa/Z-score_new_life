import pandas as pd
df = pd.read_excel('Data.xlsx')
df
data = df
data.fillna(0, inplace=True)

# Оставим только столбцы, относящиеся к количественным переменным и результатам по системе Z
z_score_columns = [
    'Кол-во бласт. Z1', 'Кол-во бласт. Z2',	'Кол-во бласт. Z3',	'Кол-во бласт. Z4',	'Кол-во бласт, Z5 (BN/SN)',	'Кол-во бласт, Z5 (1 нуклеоль)',
    'Кол-во бласт (3ББ и выше) Z1',	'Кол-во бласт (3ББ и выше) Z2',	'Кол-во бласт (3ББ и выше) Z3',	'Кол-во бласт (3ББ и выше) Z4',	'Кол-во бласт (3ББ и выше) , Z5 (BN/SN)', 'Кол-во бласт (3ББ и выше), Z5 (1 нуклеоль)',
    'Кол-во имплант. Эмб. Неизвестный паттерн',	'Кол-во имплант. Эмб. Z1',	'Кол-во имплант. Эмб. Z2', 'Кол-во имплант. Эмб. Z3', 'Кол-во имплант. Эмб. Z4', 'Кол-во имплант. Эмб., Z5 (BN/SN)', 'Кол-во имплант.Эмбр., Z5 (1 нуклеоль)',
    'Кол-во 2PN, Z1',	'Кол-во 2PN, Z2',	'Кол-во 2PN, Z3',	'Кол-во 2PN, Z4', 'Кол-во 2PN, Z5 (BN/SN)',	'Кол-во 2PN, Z5 (1 нуклеоль)'

]

# Преобразуем столбцы к числовому типу, заменяя некорректные значения на NaN
data[z_score_columns] = data[z_score_columns].apply(pd.to_numeric, errors='coerce')

# Извлечем только количественные столбцы, которые могут коррелировать с оценками Z
quantitative_columns = ['Кол-во 2PN', 'Возраст пац', 'Кол-во М2 ооц.', 'Кол-во КОК'] + z_score_columns

# Создадим новый DataFrame с необходимыми столбцами
quantitative_data = data[quantitative_columns]

# Проверим, как коррелируют оценки Z с другими количественными переменными
correlation_matrix = quantitative_data.corr()

# Отфильтруем корреляции только для Z-оценок
z_score_corr = correlation_matrix[z_score_columns].drop(z_score_columns, axis=0)

z_score_corr
